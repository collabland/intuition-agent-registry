{
  "name": "agent-registry",
  "version": "1.0.0",
  "description": "A decentralized agent registry built on the Intuition Protocol for registering, discovering, and interacting with AI agents",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "start": "node --import tsx src/server.ts",
    "dev": "tsx watch src/server.ts",
    "sync": "tsx src/sync.ts",
    "search": "tsx src/search.ts",
    "deploy": "git push heroku main",
    "logs": "heroku logs --tail",
    "get-secrets": "bash scripts/get-heroku-secrets.sh"
  },
  "keywords": [
    "agent-registry",
    "decentralized",
    "blockchain",
    "intuition-protocol",
    "ai-agents",
    "ethereum",
    "typescript",
    "express",
    "webhook",
    "api"
  ],
  "author": "Agent Registry Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/collabland/intuition-agent-registry.git"
  },
  "bugs": {
    "url": "https://github.com/collabland/intuition-agent-registry/issues"
  },
  "homepage": "https://github.com/collabland/intuition-agent-registry#readme",
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@0xintuition/graphql": "2.0.0-alpha.0",
    "@0xintuition/protocol": "2.0.0-alpha.0",
    "@0xintuition/sdk": "2.0.0-alpha.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "viem": "2.31.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^24.2.0"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ]
}